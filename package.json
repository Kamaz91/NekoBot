{
    "name": "nekobot",
    "version": "12.0.0",
    "description": "discord bot",
    "main": "./build/NekoBot.js",
    "type": "module",
    "dependencies": {
        "@discordjs/rest": "^1.3.0",
        "@types/winston": "^2.4.4",
        "axios": "^1.3.2",
        "bufferutil": "^4.0.1",
        "colors": "^1.4.0",
        "console-stamp": "^3.1.0",
        "cron": "^2.1.0",
        "discord-api-types": "^0.37.19",
        "discord.js": "^14.7.0",
        "knex": "^2.3.0",
        "lodash": "^4.17.19",
        "moment": "^2.29.4",
        "mysql2": "^2.3.3",
        "winston": "^3.8.2"
    },
    "scripts": {
        "clean": "if exist ./build rmdir /q /s build",
        "build": "npm run clean && tsc --build && tsc-alias",
        "buildnc": "tsc --build && tsc-alias",
        "buildrun": "npm run build && npm run start",
        "buildrun-debug": "npm run build && npm run start-debug",
        "start-debug": "node --experimental-json-modules --inspect-brk ./build/NekoBot.js",
        "start": "node --experimental-json-modules ./build/src/NekoBot.js",
